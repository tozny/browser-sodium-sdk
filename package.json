{
  "name": "tozny-browser-sodium-sdk",
  "version": "1.2.0-alpha.8",
  "description": "Sodium mode browser SDK for Tozny Storage (TozStore) clients.",
  "homepage": "https://github.com/tozny/browser-sodium-sdk",
  "author": {
    "name": "Tozny, LLC",
    "email": "info@tozny.com",
    "url": "https://tozny.com"
  },
  "license": "proprietary",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "keywords": [
    "encryption",
    "encrypted-store",
    "api-client"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jasmine-core": "^3.4.0",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "^2.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^2.0.2",
    "karma-testingbot-launcher": "",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "scripts": {
    "prepare": "babel lib -d dist --ignore lib/__tests__ --ignore lib/browser.js",
    "pretest": "npm run build",
    "precommit": "lint-staged",
    "test": "karma start karma.conf.js",
    "testcov": "jest --coverage",
    "prebuild": "npm run prepare",
    "build": "npx webpack --config webpack.config.js",
    "profile": "jest profile.test"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "browser": true
    },
    "rules": {
      "max-params": [
        "error",
        9
      ],
      "no-prototype-builtins": [
        "off"
      ],
      "no-await-in-loop": [
        "off"
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/tozny/browser-sodium-sdk",
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "base64url": "^3.0.1",
    "dotenv": "^6.1.0",
    "e3db-client-interface": "^0.3.0-alpha.10",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2",
    "js-md5": "^0.7.3",
    "libsodium-wrappers": "^0.7.5"
  }
}
